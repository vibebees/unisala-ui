---
import MainLayout from '@/layouts/main-layout.astro';
import HeroLanding from "@/components/sections/hero-landing-new.astro"
import BentoGrid from "@/components/sections/bentogrid.astro"
import InfoLanding from "@/components/sections/info-landing.astro"
import { infos } from "@/config/landing";
import Features from '@/components/sections/features.astro';
import Testimonials from '@/components/sections/testimonials.astro';
import Organization from '@/components/sections/organization.astro';
import GoToUnisalaPortal from '@/components/sections/goToUnisalaPortal.astro';
---
<MainLayout title ="Home">
  <HeroLanding/>
